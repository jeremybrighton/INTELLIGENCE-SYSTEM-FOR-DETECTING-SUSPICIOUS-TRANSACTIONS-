ue)\n","\n","    # 2 Add missing columns present in training\n","    for col in feature_cols:\n","        if col not in new_data_encoded.columns:\n","            new_data_encoded[col] = 0\n","\n","    # 3 Keep only training columns in correct order\n","    new_data_aligned = new_data_encoded[feature_cols].astype(float)\n","\n","    # 4 Predict probability and binary label\n","    prob = model.predict_proba(new_data_aligned)[:, 1]\n","    pred = (prob >= 0.5).astype(int)\n","\n","    # 5 Compute SHAP values safely\n","    explainer = shap.TreeExplainer(model, feature_perturbation=\"tree_path_dependent\")\n","    shap_values_all = explainer.shap_values(new_data_aligned)\n","\n","    # For binary classification, use SHAP values for positive class\n","    shap_values_pos = shap_values_all[1] if isinstance(shap_values_all, list) else shap_values_all\n","\n","    # 6 Prepare results\n","    results = pd.DataFrame({\n","        'prediction': pred,\n","        'probability': prob\n","    }, index=new_data_aligned.index)\n","\n","    return results, shap_values_pos"],"metadata":{"id":"HjS0msYNO1No","executionInfo":{"status":"ok","timestamp":1771845039877,"user_tz":-180,"elapsed":18,"user":{"displayName":"Brighton Jeremy","userId":"08890773250908098580"}}},"execution_count":270,"outputs":[]},{"cell_type":"code","source":["# Example: take first 10 rows from X_test_aligned as new transactions\n","new_transactions = X_test_aligned.iloc[:10, :].copy()"],"metadata":{"id":"K7wq_bC4U7fS"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["import shap\n","import numpy as np\n","\n","# ▖ Predict and get SHAP values for the batch\n","results, shap_values = predict_with_shap(new_transactions, model, feature_cols)\n","\n","# ▖ Get the explainer object (globally defined) for expected_value\n","# The 'explainer' object is available from a previous cell (h1uXNQ9FTRvP)\n","# Given 'explainer' is globally available and consistent, we'll use it.\n","\n","# For binary classification, TreeExplainer.expected_value is an array [base_class_0, base_class_1].\n","# Since shap_values are for the positive class (class 1), we need its corresponding base value.\n","expected_value_for_plot_global = explainer.expected_value[1] # Scalar base value for the positive class\n","\n","# ▖ Global SHAP summary plot (sample up to 500 rows)\n","n_samples = min(500, results.shape[0])\n","sample_idx = np.random.choice(results.shape[0], n_samples, replace=False)\n","\n","shap.summary_plot(\n","    shap_values[sample_idx, :],\n","    new_transactions.iloc[sample_idx, :],\n","    plot_type=\"bar\"\n",")\n","\n","# ▖ Local explanation for the first transaction (force plot)\n","# Create a NEW explainer for a SINGLE instance for robust plotting\n","single_instance_explainer = shap.TreeExplainer(model, feature_perturbation=\"tree_path_dependent\")\n","\n","# Get SHAP values for the first transaction only, passing it as a single-row DataFrame\n","# The result from shap.TreeExplainer.shap_values for binary classification\n","# with a single input will be a 3D array: (1 sample, N features, 2 outputs)\n","shap_values_single_instance_all_outputs = single_instance_explainer.shap_values(new_transactions.iloc[0:1, :])\n","\n","# Extract the SHAP values for the first sample, for all features, for the positive class (index 1)\n","shap_values_for_single_force_plot = shap_values_single_instance_all_outputs[0, :, 1] # This will be 1D: (N,)\n","\n","# Get the expected value for the positive class from this single_instance_explainer\n","expected_value_single_instance_pos = single_instance_explainer.expected_value[1]\n","\n","shap.initjs()\n","shap.plots.force(\n","    expected_value_single_instance_pos, # Positional: scalar base value for the positive class\n","    shap_values_for_single_force_plot,  # SHAP values for a single sample (1D array)\n","    new_transactions.iloc[0, :]         # Feature values for a single sample (Pandas Series for feature names)\n",")"],"metadata":{"id":"0